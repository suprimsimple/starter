{
	"name": "prometheus",
	"type": "module",
	"description": "Starter project for Simple",
	"license": "CC-BY-NC-3.0",
	"repository": {
		"type": "git",
		"url": "https://bitbucket.org/SimpleIM/prometheus"
	},
	"scripts": {
		"dev": "gulp dev",
		"csr": "npx parcel watch csr_app/index.js --dist-dir src/scripts/csr",
		"build": "gulp build",
		"p8s-tailwind": "cd ./templates && ln -s ../templates-tailwind/* . && cd ./",
		"p8s-sass": "cd ./templates && ln -s ../templates-sass/* . && cd ./",
		"p8s-reset": "find ./templates/ -type l -delete",
		"setup": "node ./setup/index.js",
		"lint": "eslint --fix templates",
		"format": "prettier --write src"
	},
	"engines": {
		"node": ">=14.16.0",
		"npm": ">=6.0.0"
	},
	"dependencies": {
		"@alpinejs/collapse": "^3.10.5",
		"@alpinejs/focus": "^3.10.5",
		"@alpinejs/intersect": "^3.10.5",
		"@babel/preset-env": "^7.20.2",
		"@fancyapps/ui": "^4.0.31",
		"alpinejs": "^3.10.5",
		"alpinejs-breakpoints": "^2.1.1",
		"babel-preset-react-app": "^3.1.2",
		"datatables.net": "^1.11.3",
		"datatables.net-fixedheader": "^3.2.1",
		"datatables.net-responsive": "^2.2.9",
		"flickity": "^3.0.0",
		"flickity-fade": "^2.0.0",
		"flickity-imagesloaded": "^2.0.0",
		"gulp-htmlmin": "^5.0.1",
		"gulp-minify-inline": "^1.1.0",
		"imagemin": "^8.0.1",
		"include-media": "^1.4.10",
		"js-cookie": "^3.0.1",
		"lazysizes": "^5.2.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rollup-plugin-visualizer": "^5.8.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-react": "^7.18.6",
		"@babel/register": "^7.18.9",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@vitejs/plugin-legacy": "^3.0.1",
		"autoprefixer": "^10.4.13",
		"core-js": "^3.26.1",
		"delete": "^1.1.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.31.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tailwindcss": "^3.8.0",
		"file-loader": "^6.2.0",
		"fs-extra": "^10.0.0",
		"glob": "^8.0.3",
		"gulp": "^4.0.2",
		"gulp-image": "^6.3.1",
		"inquirer": "^8.0.0",
		"lodash": "^4.17.21",
		"md5": "^2.3.0",
		"npm-check": "^6.0.1",
		"postcss": "^8.4.19",
		"postcss-import": "^15.0.0",
		"postcss-nesting": "^10.2.0",
		"postcss-preset-env": "^7.8.3",
		"prettier": "^2.8.2",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"process": "^0.11.10",
		"sass": "^1.56.1",
		"svgo": "^3.0.2",
		"svgstore": "^3.0.0-2",
		"tailwindcss": "^3.2.4",
		"vite": "^4.0.4",
		"vite-plugin-restart": "^0.3.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"Firefox ESR",
		"not ie 10",
		"not op_mini all",
		"not op_mob 12.1",
		"not baidu 7.12",
		"not ie_mob 10",
		"not ie_mob 11",
		"not and_uc 11.8"
	],
	"eslintConfig": {
		"parser": "babel-eslint",
		"plugins": [
			"compat"
		],
		"rules": {
			"compat/compat": "warn"
		}
	},
	"prettier": {
		"trailingComma": "es5",
		"useTabs": true,
		"tabWidth": 4,
		"semi": false,
		"singleQuote": false
	}
}
